{
  "env": {
    "es2021": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "project": "tsconfig.json",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "rules": {
    "no-param-reassign": 0,
    "import/prefer-default-export": 0
  }
}
